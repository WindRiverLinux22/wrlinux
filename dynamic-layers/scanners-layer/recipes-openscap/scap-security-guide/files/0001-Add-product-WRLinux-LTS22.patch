diff --git a/shared/checks/oval/installed_OS_is_wrlinuxlts21.xml b/shared/checks/oval/installed_OS_is_wrlinuxlts21.xml
new file mode 100644
index 0000000000..4e840fdd74
--- /dev/null
+++ b/shared/checks/oval/installed_OS_is_wrlinuxlts21.xml
@@ -0,0 +1,46 @@
+<def-group>
+	<definition class="inventory" id="installed_OS_is_wrlinuxlts21" version="1">
+		<metadata>
+			<title>Wind River Linux LTS21</title>
+			<affected family="unix">
+				<platform>multi_platform_all</platform>
+			</affected>
+			<reference ref_id="cpe:/o:windriver:wrlinux:lts21" source="CPE" />
+			<description>The operating system installed on the system is Wind River Linux LTS21</description>
+		</metadata>
+		<criteria operator="AND">
+			<criterion comment="Installed operating system is part of the unix family" test_ref="test_wrlinuxlts21_unix_family" />
+			<criterion comment="Product name is Wind River Linux" test_ref="test_wrlinuxlts21_name" />
+			<criterion comment="Wind River Linux version is LTS21" test_ref="test_wrlinuxlts21_version" />
+		</criteria>
+	</definition>
+
+	<ind:family_test check="all" check_existence="at_least_one_exists" comment="installed OS part of unix family" id="test_wrlinuxlts21_unix_family" version="1">
+		<ind:object object_ref="obj_wrlinuxlts21_unix_family" />
+		<ind:state state_ref="state_wrlinuxlts21_unix_family" />
+	</ind:family_test>
+
+	<ind:family_state id="state_wrlinuxlts21_unix_family" version="1">
+		<ind:family>unix</ind:family>
+	</ind:family_state>
+
+	<ind:family_object id="obj_wrlinuxlts21_unix_family" version="1" />
+	<ind:textfilecontent54_test check="all" check_existence="at_least_one_exists" comment="Check Wind River Linux product name" id="test_wrlinuxlts21_name" version="1">
+		<ind:object object_ref="obj_wrlinuxlts21_name" />
+	</ind:textfilecontent54_test>
+	<ind:textfilecontent54_object id="obj_wrlinuxlts21_name" version="1" comment="Check Wind River Linux product name">
+		<ind:filepath>/etc/os-release</ind:filepath>
+		<ind:pattern operation="pattern match">^NAME=.Wind[\s]+River[\s]+Linux.*$</ind:pattern>
+		<ind:instance datatype="int">1</ind:instance>
+	</ind:textfilecontent54_object>
+
+	<ind:textfilecontent54_test check="all" check_existence="at_least_one_exists" comment="Check Wind River Linux version" id="test_wrlinuxlts21_version" version="1">
+		<ind:object object_ref="obj_wrlinuxlts21_version" />
+	</ind:textfilecontent54_test>
+	<ind:textfilecontent54_object id="obj_wrlinuxlts21_version" version="1" comment="Check Wind River Linux version">
+		<ind:filepath>/etc/os-release</ind:filepath>
+		<ind:pattern operation="pattern match">^VERSION=.10\.21.*$</ind:pattern>
+		<ind:instance datatype="int">1</ind:instance>
+	</ind:textfilecontent54_object>
+
+</def-group>
